{
  "name": "Hide Youtube Recommendations",
  "version" : "1.0",
  "manifest_version": 2,
  "description" : "Hide recommendation from youtube",
  "content_scripts" : [{
    "matches": ["https://www.youtube.com/*"],
    "js": ["content.js"],
    "run_at": "document_start"
  }],
  "icons" : {
    "128": "icon-128.png"
  }
}
